				<add name="Access-Control-Allow-Methods" value="OPTIONS,POST,GET,PUT"/>
				<add name="Access-Control-Allow-Headers" value="x-requested-with"/>
				<add name="Access-Control-Allow-Credentials" value="true"/>
			</customHeaders>
		</httpProtocol>
	</system.webServer>
