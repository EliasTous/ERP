					<add fileExtension=".properties" allowed="true"/>
					<add fileExtension=".exe" allowed="true"/>
				</fileExtensions>
			</requestFiltering>
		</security>
		<staticContent>
			<mimeMap fileExtension=".properties" mimeType="application/l10n"/>
		</staticContent>
		<httpProtocol>
